    <file src="bin\DynaShape.xml" target="lib\net8.0"/>
    <file src="bin\DynaShape.customization.dll" target="lib\net8.0"/>
    <file src="bin\DynaSpace.dll" target="lib\net8.0"/>
    <file src="bin\DynaSpace.xml" target="lib\net8.0"/>
    <file src="bin\DynaSpace.customization.xml" target="lib\net8.0"/>
    <file src="bin\DynaShape_DynamoCustomization.xml" target="lib\net8.0"/>
    <file src="extra\DynaShape_ViewExtensionDefinition.xml" target="lib\net8.0"/>
    <file src="pkg.json" target="lib\net8.0"/>
  </files>
</package>